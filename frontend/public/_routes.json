{
  "version": 1,
  "include": ["/*"],
  "exclude": ["/assets/*", "/_astro/*", "/manifest.json", "/*.png", "/*.svg", "/*.ico"],
  "routes": [
    {
      "src": "/paste/.*",
      "dest": "/index.html"
    },
    {
      "src": "/file/.*",
      "dest": "/index.html"
    },
    {
      "src": "/admin",
      "dest": "/index.html"
    },
    {
      "src": "/upload",
      "dest": "/index.html"
    },
    {
      "src": "^/((?!assets/|_astro/|api/).*)$",
      "dest": "/index.html"
    }
  ]
}
